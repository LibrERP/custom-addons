<?xml version="1.0" encoding="utf-8"?>
<odoo>
     <record id="view_account_move_partner_form" model="ir.ui.view">
        <field name="name">account.move.partner.form</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <field name="partner_shipping_id" position="attributes">
                <attribute name="domain">['|', ('id', '=', partner_id), ('parent_id', '=', partner_id)]</attribute>
                <attribute name="context">{'default_type':'delivery'}</attribute>
            </field>
        </field>
     </record>
</odoo>
