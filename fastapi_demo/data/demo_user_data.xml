<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="demo_user" model="res.users"
      context="{'no_reset_password': True, 'no_reset_password': True}"
    >
        <field name="name">Demo Endpoint User</field>
        <field name="login">demo_user</field>
        <field name="groups_id" eval="[(6, 0, [])]" />
    </record>
    <record id="demo_group" model="res.groups">
        <field name="name">Demo Group</field>
        <field name="users" eval="[(4, ref('demo_user'))]" />
        <field name="implied_ids" eval="[(4, ref('fastapi.group_fastapi_endpoint_runner'))]" />
    </record>
</odoo>
